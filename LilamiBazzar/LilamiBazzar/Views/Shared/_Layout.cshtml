    <script>
        $(document).ready(function () {
            $('#loginButton').on('click', async function (e) {
                e.preventDefault();

                // Create the data object
                var data = {
                    email: $('#signinEmail').val(),
                    password: $('#signinPassword').val(),
                    rememberMe: $('#rememberMe').is(':checked')
                };

                try {
                    // Await the result from AjaxCall
                    var loginResponse = await AjaxCall("/Accounts/Signin/Login", data);

                    debugger

                    // Save the token in localStorage
                    const token = localStorage.getItem('Authorization');

                    // Call a protected resource using the Bearer token
                    var dashboard =  AjaxCallWithToken("Accounts/Dashboard" , token);

                    // Handle successful response from the protected resource
                    console.log("Dashboard Data:", dashboard);
                    // Optionally, redirect to the dashboard or handle the response
                    // window.location.href = "dashboard.html"; // Example redirection

                } catch (error) {
                    // Handle any errors that occurred during the AJAX call
                    console.error("Login failed:", error);
                }
            });
        });

        // Function to call an API with Bearer Token
         function AjaxCallWithToken(url, token) {
             $.ajax({
                url: url,
                type: 'GET', // or POST based on your API
                headers: {
                    'Authorization': 'Bearer ' + token // Attach the Bearer token
                },
                success: function (response) {
                    console.log("Authenticate");// Handle response
                    location.href = "/Accounts/Dashboard";
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    console.error("Error:", textStatus, errorThrown);
                }
            });
        }
    </script>



